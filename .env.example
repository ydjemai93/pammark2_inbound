# Clés API et paramètres de configuration

# Clé API OpenAI pour l'API Realtime et Audio
OPENAI_API_KEY=your_openai_api_key

# Clé API Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token

# Numéro de téléphone Twilio (format international)
TWILIO_PHONE_NUMBER=+15017122661

# Domaine public de votre application (sans protocole, ex: yourdomain.ngrok.io)
SERVER=yourdomain.ngrok.io

# Port d'exécution de l'application
PORT=5050
